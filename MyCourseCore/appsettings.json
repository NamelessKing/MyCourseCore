{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=Data/MyCourse.db; Mode=ReadWrite;"
  },
  "Courses": {
    "PerPage": 10,
    "Order": {
      "By": "Rating",
      "Ascending": false,
      "Allow": [ "Title", "Rating", "CurrentPrice" ]
    }
  }
}
